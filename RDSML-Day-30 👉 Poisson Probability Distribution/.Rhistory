#poisson distribution
poisson <- function(x, lambda) {
if (lambda <= 0) {
stop("Lambda must be positive")
}
if (x < 0 || x != floor(x)) {
return(0)
}
return((lambda^x * exp(-lambda)) / factorial(x))
}
# cumulative distribution function for Poisson
poisson_cdf <- function(x, lambda) {
if (lambda <= 0) {
stop("Lambda must be positive")
}
if (x < 0 || x != floor(x)) {
return(0)
}
sum(sapply(0:x, function(k) poisson(k, lambda)))
}
